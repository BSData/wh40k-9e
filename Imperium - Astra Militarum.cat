      </characteristics>
    </profile>
    <profile id="6338-ae1c-93c9-2e89" name="Scout Vehicle" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">

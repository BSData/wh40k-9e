    <profile id="fdc3-2a0f-e726-f47c" name="Emergency Plasma Vents" book="Codex: Astra Militarum" page="114" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">

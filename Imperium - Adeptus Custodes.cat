    <profile id="ac77-6fb7-aa8e-6f46" name="Martial Inspiration" publicationId="1f0f-230b-a715-7d27" page="92" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

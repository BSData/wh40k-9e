        </profile>
        <profile id="ec05-6187-5619-7930" name="Battle Protocols: Aegis" book="Codex: Adeptus Mechanicus" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">

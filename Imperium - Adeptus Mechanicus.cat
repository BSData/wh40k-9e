      </characteristics>
    </profile>
    <profile id="a393-0ab6-7e6b-d526" name="First-Hand Field Testing" publicationId="ebe8-544e-pubN65537" hidden="false" typeId="7df1-74bd-09c9-492c" typeName="Warlord Trait">

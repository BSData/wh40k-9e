    </profile>
    <profile id="7c84-9fec-5e37-9734" name="Legendary Fighter" publicationId="ebe8-544e-pubN79743" hidden="false" typeId="7df1-74bd-09c9-492c" typeName="Warlord Trait">
      <characteristics>
        <characteristic name="Description" typeId="696e-7a95-ff17-7c91">If this Warlord charges in the Charge phase, add 1 to their Attacks characteristic until the end of the ensuing Fight phase.</characteristic>
      </characteristics>
    </profile>
    <profile id="79e8-4d72-2956-e80f" name="Tenacious Survivor" publicationId="ebe8-544e-pubN79743" hidden="false" typeId="7df1-74bd-09c9-492c" typeName="Warlord Trait">
      <characteristics>

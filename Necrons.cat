          </characteristics>
        </profile>
        <profile id="194f-926e-8db9-3565" name="Transdimensional Explosion (High Power)" publicationId="61c6-c8c5-pubN106894" hidden="false" typeId="1c4c-8a58-c471-95e3" typeName="Explosion">
          <characteristics>

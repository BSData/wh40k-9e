    <selectionEntry id="bd3c-6d2c-f917-ebaa" name="[Reference] Necron Secondary Objectives (All)" publicationId="61c6-c8c5-pubN65537" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="70bc-62aa-d3af-bd2a" type="max"/>
      </constraints>

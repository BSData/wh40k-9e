            <characteristic name="BS" typeId="5ea3-6edf-c811-3d4f">4+</characteristic>
            <characteristic name="Portal of Exile" typeId="5b3c-e43b-dde3-9a6f">5+</characteristic>
          </characteristics>
        </profile>
        <profile id="c4c7-455e-7227-3bd7" name="Monolith Track 1" publicationId="61c6-c8c5-pubN65537" hidden="false" typeId="4ef8-90f4-8b5a-2352" typeName="Wound Track (Monolith)">
          <characteristics>
            <characteristic name="Remaining W" typeId="e69c-8f46-015d-58a6">11-20+</characteristic>

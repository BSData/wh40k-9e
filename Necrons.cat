    <profile id="2008-2de0-93b7-fd19" name="Transdimensional Beamer" publicationId="61c6-c8c5-pubN65537" hidden="false" typeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" typeName="Weapon">

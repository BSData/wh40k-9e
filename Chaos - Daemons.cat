    <selectionEntry id="bc1f-c1df-5fe8-3cc8" name="Skull Altar" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditionGroups>
            <conditionGroup type="and">
              <conditions>
                <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="c047-a750-7057-082f" type="equalTo"/>

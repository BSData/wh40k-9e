              <profiles>
                <profile id="86b2-75d9-0ed3-23ac" name="Hyper-aggression" publicationId="7150-5917-pubN65537" page="53" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

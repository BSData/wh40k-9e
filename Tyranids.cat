    <profile id="339f-b2b7-bd45-91f9" name="Floating Death (Spore Mine)" publicationId="7150-5917-pubN65537" page="111" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

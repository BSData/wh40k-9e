    <profile id="bc40-384b-8bf8-f27e" name="Vanguard Predator" publicationId="7150-5917-pubN65537" page="" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

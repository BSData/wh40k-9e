    <profile id="3109-02b3-0be5-96d6" name="Emboldended by Prayer (Aura)" publicationId="d124-b283-pubN72234" page="97" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

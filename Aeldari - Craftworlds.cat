    <profile id="7d02-ee83-26bd-fa96" name="Will of Asuryan (Blessing)" publicationId="1f62-db93-63f4-4dc7" page="122" hidden="false" typeId="ae70-4738-0161-bec0" typeName="Psychic Power">

    <profile id="3bb5-a77b-67c5-aa61" name="Executioner (Witchfire)" publicationId="1f62-db93-63f4-4dc7" page="122" hidden="false" typeId="ae70-4738-0161-bec0" typeName="Psychic Power">
      <characteristics>

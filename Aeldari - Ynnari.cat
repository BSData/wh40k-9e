                <selectionEntry id="48ca-c06c-952c-41ae" name="Meticulous Flayers" hidden="false" collective="false" import="true" type="upgrade">
                  <constraints>
                    <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="753f-6fff-cd84-1f03" type="max"/>

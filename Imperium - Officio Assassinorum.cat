    <selectionEntry id="b2b2-a0a3-6984-ee22" name="Show Stratagems" hidden="false" collective="false" import="false" type="upgrade">
      <categoryLinks>
        <categoryLink id="a16b-6fd6-c2ba-6d66" name="New CategoryLink" hidden="false" targetId="fcff-0f21-93e6-1ddc" primary="true"/>

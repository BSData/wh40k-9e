        <selectionEntry id="9446-c083-c837-0c7a" name="6. Loathsome Grace" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="11c2-cbb8-e6ba-1164" type="max"/>
          </constraints>
          <profiles>
            <profile id="b22b-2976-2833-a096" name="Loathsome Grace" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

        <profile id="6380-d18f-abbf-6b05" name="Sworn Protectors" publicationId="d124-b283-pubN72234" page="85" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

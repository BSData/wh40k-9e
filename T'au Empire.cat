    <profile id="c109-e3f5-6759-671e" name="Armoured Aggression" publicationId="6d04-0cc1-aee0-9041" page="123" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

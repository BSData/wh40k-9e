    <profile id="1f4d-b8cf-598a-2199" name="High-altitude Manoeuvres" publicationId="6d04-0cc1-aee0-9041" page="102" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

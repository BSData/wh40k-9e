    <profile id="a466-b700-0df7-acca" name="XV02 Pilot Battlesuit" publicationId="6d04-0cc1-aee0-9041" page="107" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

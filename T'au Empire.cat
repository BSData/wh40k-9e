    <profile id="464c-4f9d-8cf5-bbbf" name="Assassin" publicationId="7573-8d1b-acdf-2de8" page="205" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

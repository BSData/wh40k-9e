    <profile id="6b11-29a5-e421-0a49" name="Fighting Retreat (Aura)" publicationId="6d04-0cc1-aee0-9041" page="106" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

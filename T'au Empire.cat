    <profile id="3a0d-bc1c-df09-4b87" name="Hover Jet (Manta Dropship)" publicationId="c0bb-c0cd-pubN67762" page="57" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

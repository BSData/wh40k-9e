    <profile id="896e-8a86-04ab-363c" name="Pulse Bombs" publicationId="6d04-0cc1-aee0-9041" page="125" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

    <profile id="cb8d-e1e4-3ca6-9d20" name="Chapter Master (Blood Angels)" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
      <characteristics>
        <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">You can re-roll failed hit rolls for friendly BLOOD ANGELS units within 6&quot; of Commander Dante.</characteristic>
      </characteristics>
    </profile>
    <profile id="46cc-edf3-5fd4-4a8b" name="Chapter Master (Flesh Tearers)" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
      <characteristics>
        <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">You can re-roll failed hit rolls for friendly FLESH TEARERS units within 6&quot; of Gabriel Seth.</characteristic>
      </characteristics>
    </profile>
    <profile id="3f03-d1d8-e276-7953" name="Lord of Slaughter" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
      <characteristics>
        <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">Roll a D6 each time a friendly FLESH TEARERS unit finishes its move within 6&quot; of Gabriel Seth when it consolidates; on a 6 that unit can immediately fight for a second and final time.</characteristic>
      </characteristics>
    </profile>
    <profile id="7de1-49cc-8dad-c0b8" name="Whirlwind of Gore" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

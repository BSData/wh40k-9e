            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="5151-7dd9-1fba-0553" name="5. Target Priority" hidden="false" collective="false" import="true" type="upgrade">
          <profiles>
            <profile id="919c-0099-b9d2-50e7" name="Target Priority" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
              <characteristics>
                <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">At the start of your Shooting phase, select one friendly BLOOD ANGELS PHOBOS unit within 3&quot; of this Warlord; until the end of that phase, when resolving an attack made with a ranged weapon by a model in that unit, add 1 to the hit roll.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name=" PL" typeId="e356-c769-5920-6e14" value="0.0"/>

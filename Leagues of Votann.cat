    <selectionEntry id="c813-55ee-2327-5b11" name="Nomad Strategist (Trans-Hyperian Alliance)" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="parent" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="72bc-19e5-1685-d2b9" type="notInstanceOf"/>

      </costs>
    </selectionEntry>
    <selectionEntry id="69b1-ba3f-bb05-3dfd" name="6. Grim Demeanor (Aura)" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="9bd3-ecca-cd71-08fd" type="max"/>

    <profile id="74b7-5b3f-ecda-f766" name="Death Korps Voice of Command" publicationId="91ff-e116-pubN65631" page="56" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
      <characteristics>
        <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">This unit may issue order to those under their command at the start of any of their Shooting phases. Orders may only be issued to INFANTRY or CAVALRY units within 6&quot; of this unit that have the DEATH KORPS OF KRIEG keyword. To issue an order, pick a target unit and choose which order you wish to issue from the table below. A unit may only be affected by one order per turn.</characteristic>
      </characteristics>
    </profile>
    <profile id="ef44-06c5-0f92-e193" name="The Cult of Sacrifice" publicationId="91ff-e116-pubN65631" page="56" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="b1d8-af72-eed4-8a5a" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <characteristics>
        <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">This unit ignores models slain in the Shooting phase when taking Morale tests.</characteristic>
      </characteristics>
    </profile>
    <profile id="3f80-6854-b4b4-b594" name="Refractor Field" publicationId="91ff-e116-pubN65631" page="57" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

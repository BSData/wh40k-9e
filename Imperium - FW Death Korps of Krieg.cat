    <profile id="d46f-c330-0453-c845" name="Without Mercy" hidden="false" typeId="df35-fad2-be79-e105" typeName="Death Korps Orders">
      <characteristics>
        <characteristic name="Effect" typeId="b39b-0a13-b7c8-b32f">All lasguns and hot-shot lasguns in the ordered unit change their Type to Pistol 2 until the end of the turn.</characteristic>

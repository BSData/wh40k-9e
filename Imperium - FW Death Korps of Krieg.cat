    <profile id="6951-2c00-7c8c-2f10" name="DKOK Orders" book="Imperial Armour - Index - Forces of the Astra Militarum" page="56" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">

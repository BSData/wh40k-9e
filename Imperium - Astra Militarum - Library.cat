      </characteristics>
    </profile>
    <profile id="54ce-8da3-62f7-ba14" name="Multiple Rocket Pod" publicationId="53e9d88f--pubN88319" page="0" hidden="false" typeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" typeName="Weapon">

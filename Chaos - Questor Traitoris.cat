            <constraint field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="0fea-83d8-214b-ecad" type="max"/>
          </constraints>
          <profiles>
            <profile id="aca5-ea81-1532-f656" name="Warp-haunted Hull" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
              <characteristics>
                <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">This Warlord can attempt to deny one psychic power in your opponent&apos;s Psychic phase in the same manner as a PSYKER. In addition, when this Warlord would lose a wound as a result of a mortal wound in the Psychic phase, roll one D6; on a 5+ that wound is not lost.</characteristic>
              </characteristics>
            </profile>
          </profiles>
        </selectionEntry>
        <selectionEntry id="88ea-d728-b2cf-a622" name="5. Eager for the Kill" hidden="false" collective="false" type="upgrade">

    <profile id="7f8e-7d4a-13a0-e8ab" name="Explodes (6+/6&quot;/D3)" publicationId="83b2-e602-pubN65537" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">

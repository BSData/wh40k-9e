        </profile>
        <profile id="c148-28b0-3d06-8c9d" name="2. Enhanced Resilience" hidden="false" typeId="d01c-5195-e332-7b24" typeName="Genomic Enhancement">
          <characteristics>
            <characteristic name="Bonus" typeId="b2a1-4c21-5bbd-9775">+1 Toughness</characteristic>
          </characteristics>
        </profile>
        <profile id="f549-0c46-02bd-45b2" name="3. Enhanced Aggression" hidden="false" typeId="d01c-5195-e332-7b24" typeName="Genomic Enhancement">

    <profile id="9420-2013-27d9-c8cd" name="Explodes (6+/3&quot;/1)" publicationId="83b2-e602-pubN65537" page="" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
